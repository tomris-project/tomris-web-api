{
  "compilerOptions": {
     "jsx": "react-jsxdev",
     "baseUrl": ".",
     "outDir": "./dist",
     "target": "ES5",
     "forceConsistentCasingInFileNames": true,
     "declaration": true,
     "incremental": true,
     "noImplicitAny": true,
     "esModuleInterop": true,
     "strictNullChecks": false,
     "strict": true,
     "isolatedModules": true,
     "resolveJsonModule": true
  },
  "include": [
    "src/**/*" 
  ]
}